    <hlayout>
        <vlayout border="true">
            <slider>MySlider</slider>
            <t_group min_count="1" max_count="1" value="1">
                <title>MySelectionGroup</title>
                <choice>
                    <contentId>item01</contentId>
                </choice>
                <choice>
                    <contentId>item02</contentId>
